Cell[CellGroupData[{

Cell["The Dirac Equation", "Section",
 CellChangeTimes->{{3.6991666101454372`*^9, 3.699166615434822*^9}}],

Cell["The Dirac equation can be written as", "Text",
 CellChangeTimes->{{3.699166726543433*^9, 3.699166735218837*^9}}],

Cell[BoxData[
 FormBox[
  RowBox[{"\t\t\t\t\t", 
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{"\[ImaginaryI]", " ", "c", " ", "\[HBar]", " ", 
       RowBox[{
        OverscriptBox["\[PartialD]", "_"], " ", "\[CapitalPsi]"}], " ", 
       SubscriptBox["\[Sigma]", "3"]}], "-", " ", 
      RowBox[{"c", " ", "e", " ", 
       OverscriptBox["A", "_"], " ", "\[CapitalPsi]"}]}], " ", "=", " ", 
     RowBox[{"m", " ", 
      SuperscriptBox["c", "2"], "\[CapitalPsi]"}]}], ","}]}], 
  TraditionalForm]], "DisplayFormulaNumbered",
 CellChangeTimes->{{3.699166777004177*^9, 3.6991667842635937`*^9}, {
   3.699166827595642*^9, 3.6991669866084967`*^9}, 3.699167055727892*^9, 
   3.6991673068800697`*^9, {3.699167441576892*^9, 3.699167442043023*^9}, {
   3.699204495529252*^9, 
   3.69920449727516*^9}},ExpressionUUID->"f14233f0-cb77-4e5e-91fb-\
685b932b5b67"],

Cell[TextData[{
 "where the spinor ",
 Cell[BoxData[
  FormBox["\[CapitalPsi]", TraditionalForm]],
  FormatType->"TraditionalForm"],
 " is related to the standard Dirac spinor (Dirac basis) as"
}], "Text",
 CellChangeTimes->{{3.699167443400629*^9, 3.699167534094317*^9}}],

Cell[BoxData[
 RowBox[{"\t\t\t\t\t\t\t\t", 
  RowBox[{
   RowBox[{
    RowBox[{"(", GridBox[{
       {
        SubscriptBox["\[Psi]", "1"]},
       {
        SubscriptBox["\[Psi]", "2"]},
       {
        SubscriptBox["\[Psi]", "3"]},
       {
        SubscriptBox["\[Psi]", "4"]}
      }], ")"}], " ", "\[DoubleLongLeftRightArrow]", 
    RowBox[{"(", GridBox[{
       {
        RowBox[{
         SubscriptBox["\[Psi]", "1"], "+", 
         SubscriptBox["\[Psi]", "3"]}], 
        RowBox[{
         RowBox[{"-", 
          SuperscriptBox[
           SubscriptBox["\[Psi]", "2"], "*"]}], "+", 
         SuperscriptBox[
          SubscriptBox["\[Psi]", "4"], "*"]}]},
       {
        RowBox[{
         SubscriptBox["\[Psi]", "2"], "+", 
         SubscriptBox["\[Psi]", "4"]}], 
        RowBox[{"   ", 
         RowBox[{
          SuperscriptBox[
           SubscriptBox["\[Psi]", "1"], "*"], "-", 
          SuperscriptBox[
           SubscriptBox["\[Psi]", "3"], "*"]}]}]}
      }], ")"}]}], "."}]}]], "DisplayFormulaNumbered",
 CellChangeTimes->{{3.699167553249065*^9, 3.699167588592799*^9}, {
  3.699167813613039*^9, 3.69916799104105*^9}}],

Cell["In the Weyl column representation we have", "Text",
 CellChangeTimes->{{3.6991680840721273`*^9, 3.6991681031110573`*^9}}],

Cell[BoxData[
 RowBox[{"\t\t\t\t\t\t\t\t", 
  RowBox[{
   RowBox[{
    RowBox[{"(", GridBox[{
       {
        SubscriptBox["\[Psi]", "1"]},
       {
        SubscriptBox["\[Psi]", "2"]},
       {
        SubscriptBox["\[Psi]", "3"]},
       {
        SubscriptBox["\[Psi]", "4"]}
      }], ")"}], " ", "\[DoubleLongLeftRightArrow]", 
    SqrtBox["2"]}], 
   RowBox[{
    RowBox[{"(", GridBox[{
       {
        SubscriptBox["\[Psi]", "3"], 
        RowBox[{"-", 
         SuperscriptBox[
          SubscriptBox["\[Psi]", "2"], "*"]}]},
       {
        SubscriptBox["\[Psi]", "4"], 
        RowBox[{"   ", 
         SuperscriptBox[
          SubscriptBox["\[Psi]", "1"], "*"]}]}
      }], ")"}], "."}]}]}]], "DisplayFormulaNumbered",
 CellChangeTimes->{{3.699167553249065*^9, 3.699167588592799*^9}, {
  3.699167813613039*^9, 3.69916799104105*^9}, {3.6991681182691936`*^9, 
  3.6991681502336607`*^9}}],

Cell["The current is given by", "Text",
 CellChangeTimes->{{3.69916841868124*^9, 3.699168425252654*^9}}],

Cell[BoxData[
 FormBox[
  RowBox[{"\t\t\t\t\t", 
   RowBox[{
    RowBox[{"J", " ", "=", "  ", 
     RowBox[{"\[CapitalPsi]", " ", 
      SuperscriptBox["\[CapitalPsi]", "\[Dagger]"]}]}], ","}]}], 
  TraditionalForm]], "DisplayFormulaNumbered",
 CellChangeTimes->{{3.699166777004177*^9, 3.6991667842635937`*^9}, {
   3.699166827595642*^9, 3.6991669866084967`*^9}, 3.699167055727892*^9, 
   3.6991673068800697`*^9, {3.699167441576892*^9, 3.699167442043023*^9}, {
   3.699168507514374*^9, 3.699168540835451*^9}}],

Cell["which is similar to the formula for the proper velocity", "Text",
 CellChangeTimes->{{3.6991685527413483`*^9, 3.699168574187325*^9}, {
  3.699168633083817*^9, 3.699168639440345*^9}}],

Cell[BoxData[
 FormBox[
  RowBox[{"\t\t\t\t\t", 
   RowBox[{
    RowBox[{"u", " ", "=", "  ", 
     RowBox[{"\[CapitalLambda]", " ", 
      SuperscriptBox["\[CapitalLambda]", "\[Dagger]"]}]}], ","}]}], 
  TraditionalForm]], "DisplayFormulaNumbered",
 CellChangeTimes->{{3.699166777004177*^9, 3.6991667842635937`*^9}, {
   3.699166827595642*^9, 3.6991669866084967`*^9}, 3.699167055727892*^9, 
   3.6991673068800697`*^9, {3.699167441576892*^9, 3.699167442043023*^9}, {
   3.699168507514374*^9, 3.699168540835451*^9}, {3.6991686014078608`*^9, 
   3.69916862330792*^9}, {3.699168795784264*^9, 3.6991687965802526`*^9}}],

Cell["but in the case of the Dirac spinor we have", "Text",
 CellChangeTimes->{{3.6991687983718367`*^9, 3.6991688153142653`*^9}}],

Cell[BoxData[
 FormBox[
  RowBox[{"\t\t\t\t\t ", 
   RowBox[{
    RowBox[{
     RowBox[{"Det", "[", "\[CapitalPsi]", "]"}], "=", " ", 
     SuperscriptBox["e", "\[ImaginaryI]\[Beta]"]}], ","}]}], 
  TraditionalForm]], "DisplayFormulaNumbered",
 CellChangeTimes->{{3.699166777004177*^9, 3.6991667842635937`*^9}, {
   3.699166827595642*^9, 3.6991669866084967`*^9}, 3.699167055727892*^9, 
   3.6991673068800697`*^9, {3.699167441576892*^9, 3.699167442043023*^9}, {
   3.699168507514374*^9, 3.699168540835451*^9}, {3.699168881980093*^9, 
   3.699168916016778*^9}}],

Cell[TextData[{
 "where \[Beta] is the Yvon-",
 StyleBox["Takabayashi",
  IgnoreSpellCheck->True],
 " angle where \[Beta]=0 for particles and \[Beta] = -\[Pi]  for  \
